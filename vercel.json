{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/report/order",
            "destination": "/api/report/order"
        },
        {
            "source": "/report/order/pdf",
            "destination": "/api/report/order/pdf"
        },
        {
            "source": "/(.*)",
            "destination": "/api/$1"
        }
    ]
}